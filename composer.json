{
    "name": "haevol/laravel-openproject-feedback",
    "description": "Laravel package for collecting user feedback and automatically creating work packages in OpenProject",
    "keywords": ["laravel", "openproject", "feedback", "bug-tracking", "issue-tracking"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Haevol",
            "email": "info@haevol.org"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Haevol\\OpenProjectFeedback\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Haevol\\OpenProjectFeedback\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Haevol\\OpenProjectFeedback\\OpenProjectFeedbackServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

